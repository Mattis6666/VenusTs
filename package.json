        "@types/node-fetch": "^2.5.5",
        "@types/ws": "^7.2.3",
        "config": "^3.3.0",
        "discord.js": "^12.0.2",
        "discord.js-minesweeper": "^1.0.5",
        "mongoose": "^5.9.6",
        "node-fetch": "^2.6.0",
        "typescript": "^3.8.3"
