{
    "name": "unnamed-ts",
    "version": "1.0.0",
    "main": "index.ts",
    "scripts": {
        "test": "tsc --noEmit --incremental=false && eslint '*/**/*.ts'",
        "start": "node out/src/index",
        "dev": "tsc && export NODE_ENV=development && npm run start",
        "production": "tsc && export NODE_ENV=production && npm run start",
        "build": "tsc",
        "reinstall": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && yarn install",
        "cleanup": "rm -rf out/*"
    },
    "author": "",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "@types/node": "^13.9.3",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-prettier": "^3.1.2"
    },
    "dependencies": {
        "@types/config": "^0.0.36",
        "@types/node-fetch": "^2.5.5",
        "@types/ws": "^7.2.3",
        "config": "^3.3.0",
        "discord.js": "^12.0.2",
        "discord.js-minesweeper": "^1.0.5",
        "mongoose": "^5.9.6",
        "node-fetch": "^2.6.0",
        "typescript": "^3.8.3"
    }
}
