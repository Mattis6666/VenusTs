{
    "name": "unnamed-ts",
    "version": "1.0.0",
    "main": "index.ts",
    "scripts": {
        "build": "tsc",
        "start": "node out/src/index",
        "dev": "npm run build && export NODE_ENV=development && npm run start",
        "production": "npm run build && export NODE_ENV=production && npm run start",
        "reinstall": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && yarn install",
        "cleanup": "rm -rf out/*",
        "lint": "eslint . --ext .ts --fix"
    },
